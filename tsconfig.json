{
    "compilerOptions": {
     "outDir": "./dist/",
     "allowJs": true,
     "allowSyntheticDefaultImports": true,
     "baseUrl": "src",
     "sourceMap": true,
     "esModuleInterop": true,
     "forceConsistentCasingInFileNames": true,
     "jsx": "react",
     "lib": [
      "dom",
      "dom.iterable",
      "esnext"
     ],
     "module": "esnext",
     "moduleResolution": "node",
     "noImplicitAny": true,
     "resolveJsonModule": true,
     "skipLibCheck": true,
     "strict": true,
     "target": "es5",
     "isolatedModules": false
    },
    "include": [
     "src/"
    ]
   }